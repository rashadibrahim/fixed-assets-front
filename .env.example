# Backend API Configuration
VITE_API_URL=http://127.0.0.1:5000
VITE_API_TIMEOUT=5000

# Development Configuration
VITE_ENVIRONMENT=development
VITE_PORT=5173

# Authentication
VITE_JWT_SECRET=your_jwt_secret_here
VITE_JWT_EXPIRES_IN=24h

# Database Configuration (if applicable)
VITE_DB_HOST=localhost
VITE_DB_PORT=5432
VITE_DB_NAME=fixed_assets_db
VITE_DB_USER=your_db_user
VITE_DB_PASSWORD=your_db_password

# File Upload Configuration
VITE_UPLOAD_MAX_SIZE=10485760
VITE_UPLOAD_ALLOWED_TYPES=image/jpeg,image/png,image/gif,application/pdf

# Feature Flags
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=false

# Application Settings
VITE_APP_NAME=Fixed Assets Management
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Fixed Assets Management System

# Security
VITE_CORS_ORIGIN=http://localhost:5173
VITE_SECURE_COOKIES=false

# Email Configuration (if needed)
VITE_SMTP_HOST=smtp.gmail.com
VITE_SMTP_PORT=587
VITE_SMTP_USER=your_email@gmail.com
VITE_SMTP_PASSWORD=your_app_password

# File Storage
VITE_STORAGE_TYPE=local
VITE_STORAGE_PATH=./uploads

# Logging
VITE_LOG_LEVEL=info
VITE_LOG_FILE=./logs/app.log